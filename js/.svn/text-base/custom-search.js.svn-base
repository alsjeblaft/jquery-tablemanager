    $(document).ready(function(){
      var suche_suchstring = [
        "search 1 (indication)",
        "search 1 (trade name)",
        "search 2 (generic name)",
        "search 2 (trade name)",
      ]

      var suche_zielstring = [];
      suche_zielstring["search 1 (indication)"] = "/result/1";
      suche_zielstring["search 1 (trade name)"] = "/result/1";
      suche_zielstring["search 2 (generic name)"] = "/result/2";
      suche_zielstring["search 2 (trade name)"] = "/result/2";

      $('#search').autocomplete({
        source: suche_suchstring
      });

      $('.modSearch .other').click(function() {
        $('.modSearch .first').attr('checked', false);
      });
   
      $('#go').click(function() {
        if (suche_zielstring[$('#search').val()]) {
          open(suche_zielstring[$('#search').val()]);
        } else {
          alert('Kein Suchbegriff ausgewählt');
        }
      });

    })
